{
    "name": "rhoban/plater",
    "binaries": ["plater/build", "gui"],
    "build":
        [
            "cd plater",
            "mkdir -p build",
            "cd build",
            "cmake -DCMAKE_BUILD_TYPE=Release ..",
            "make",
            "cd ../../gui",
            "qmake .",
            "make"
        ]

}
